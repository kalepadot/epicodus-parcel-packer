@using Shipping.Models

<h1> Your shipping, sir.</h1>
<hr>

@if (Model.Count == 0)
{
  <h1>Go ship yourself!</h1>
}
else
{
  @foreach (Parcel parcel in Model)
  {
  <p>Dimensions are @parcel.Height x @parcel.Width x @parcel.Length </p>
  <p>Total volume: @parcel.Volume Total weight: @parcel.Weight </p>
  <p>Your cost is $@parcel.Cost</p>
  }

}

<a href= '/'>Try again, it was fun.</a>